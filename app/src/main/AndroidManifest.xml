<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Tema2"
        tools:targetApi="31">
        <activity
            android:name=".informasi"
            android:label="Tentang Aplikasi"
            android:theme="@style/Tema1"
            android:exported="false" />
        <activity
            android:name=".inputpesanan"
            android:label="Pemesanan"
            android:theme="@style/Tema1"
            android:exported="false" />
        <activity
            android:name=".menu"
            android:label="Menu Cafe JMP"
            android:theme="@style/Tema1"
            android:exported="false" />
        <activity
            android:name=".listpesanan"
            android:label="Data Pemesanan"
            android:theme="@style/Tema1"
            android:exported="false" />
        <activity
            android:name=".splashscreen"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="CAFE JMP"
            android:theme="@style/Tema1" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>